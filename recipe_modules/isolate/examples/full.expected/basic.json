[
  {
    "cmd": [],
    "name": "ensure_isolate"
  },
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[START_DIR]/cipd/luci/isolate",
      "-ensure-file",
      "infra/tools/luci/isolate/linux-amd64 release",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "infra_step": true,
    "name": "ensure_isolate.ensure_installed",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-release---------\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"infra/tools/luci/isolate/linux-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/cipd/luci/isolate/isolate",
      "archive",
      "-isolate-server",
      "isolateserver.appspot.com",
      "-isolate",
      "{\"variables\": {\"files\": [\"test\"]}}",
      "-isolated",
      "[TMP_BASE]/test.isolated",
      "-namespace",
      "default-gzip",
      "-dump-json",
      "/path/to/tmp/json"
    ],
    "name": "archive",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"test\": \"[dummy hash]\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/cipd/luci/isolate/isolate",
      "version"
    ],
    "name": "isolate version"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]